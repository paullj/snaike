{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "ES2019",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "removeComments": true,
    "forceConsistentCasingInFileNames": true,
    "importsNotUsedAsValues": "error",
    "composite": true
  }
}
